RewriteRule brands-add                                             pages/brands/brands-add.php [L,NC,QSA]
RewriteRule brands/pages/(.*)                                      pages/brands/brands-list.php?page=$1 [L,NC,QSA]
RewriteRule ^brands                                                pages/brands/brands-list.php [L,NC,QSA]


